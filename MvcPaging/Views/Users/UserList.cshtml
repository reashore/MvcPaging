
@model List<User>;

<h1>User List</h1>

<table class="table table-striped table-bordered table-sm">
    <tr>
        <th>UserId</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Created Date</th>
    </tr>
    @foreach (User user in Model)
    {
        <tr>
            <td>@user.UserId</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.CreatedDate.ToShortDateString()</td>
        </tr>
    }
</table>